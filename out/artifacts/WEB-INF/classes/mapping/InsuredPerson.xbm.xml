<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="ua.profitsoft.strymeneshenko.data">
    <class name="InsuredPerson" table="insuredperson">
        <id name="id" type="java.lang.Long" column="id">
            <generator class="native"/>
        </id>
        <property name="firstName" column="firstName"/>
        <property name="lastName" column="lastName"/>
        <property name="dateOfBirth" column="dateOfBirth"/>
        <property name="cost" column="cost"/>
        <property name="identificationNumber" column="identificationNumber"/>
    </class>
</hibernate-mapping>